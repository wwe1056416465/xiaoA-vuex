<view class="uni-tab-bar" bind:__l="__l"><view class="temlist_top"><view class="tem_top">共<text>{{totalFile}}</text>条录音,
			已完成<text class="text_gr">{{totalRecordFile}}</text>条,
			未完成<text class="text_dg">{{unRecordFile}}</text>条</view><view class="tabs"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{[(cur==0)?'tab_active':'']}}" bindtap="__e">主流程</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{[(cur==1)?'tab_active':'']}}" bindtap="__e">问题流程</view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="{{[(cur==2)?'tab_active':'']}}" bindtap="__e">特殊流程</view></view><view class="space10"></view><view hidden="{{!(cur==0)}}" class="content"><block wx:for="{{templates}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tem_List',['$0','$1'],['callflowid',[['templates','',index,'contextid']]]]]]]}}" class="tem_list" bindtap="__e"><view class="tem_left">{{item.contextName}}</view><view hidden="{{!(item.isComplete=='未完成')}}" class="tem_right text_dg">{{item.isComplete}}</view><view hidden="{{!(item.isComplete=='已完成')}}" class="tem_right text_gr">{{item.isComplete}}</view></view></block></view><view hidden="{{!(cur==1)}}" class="content"><block wx:for="{{qustions}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view data-event-opts="{{[['tap',[['tem_questions',['$0','$1',index,'$2'],[[['qustions','',index,'id']],[['qustions','',index,'callflowid']],[['qustions','',index,'type']]]]]]]}}" class="tem_list" bindtap="__e"><view class="tem_left">{{item.name}}</view><view hidden="{{!(item.isComplete=='未完成')}}" class="tem_right text_dg">{{item.isComplete}}</view><view hidden="{{!(item.isComplete=='已完成')}}" class="tem_right text_gr">{{item.isComplete}}</view></view></view></block></view><view hidden="{{!(cur==2)}}" class="content"><block wx:for="{{stuTem}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tem_specli',['$0','$1',index,'$2'],[[['stuTem','',index,'id']],[['stuTem','',index,'callflowid']],[['stuTem','',index,'type']]]]]]]}}" class="tem_list" bindtap="__e"><view class="tem_left">{{item.name}}</view><view hidden="{{!(item.isComplete=='未完成')}}" class="tem_right text_dg">{{item.isComplete}}</view><view hidden="{{!(item.isComplete=='已完成')}}" class="tem_right text_gr">{{item.isComplete}}</view></view></block></view></view><view class="temlist_bottom"><view data-event-opts="{{[['tap',[['audit']]]]}}" bindtap="__e">提交审核</view><view data-event-opts="{{[['tap',[['Jverify']]]]}}" bindtap="__e">审核列表</view></view></view>