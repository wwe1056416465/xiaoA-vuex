.animation1 {
	  width: 110px;
	  height: 110px;
	  border-radius: 50%;
	  border: 1px solid #3691FF;
	  position: absolute;
	  top: -10px;
	  left: -10px;
	  -webkit-animation: animation1 0.5s ease 0s infinite alternate;
}
.animation2 {
	  width: 100px;
	  height: 100;
	  border-radius: 50%;
	  border: 1px solid #3691FF;
	  position: absolute;
	  top: -5px;
	  left: -5px;
	  -webkit-animation: animation2 0.5s ease 0s infinite alternate;
}
.record-state{
		margin: 0 10px;
		color: #6ac96a;
}
@-webkit-keyframes animation1 {
0% {
		opacity: 0;
		width: 90px;
		height: 90px;
		position: absolute;
		top: 0;
		left: 0;
}
100% {
		opacity: 0.6;
		width: 110px;
		height: 110px;
}
}
@keyframes animation1 {
0% {
		opacity: 0;
		width: 90px;
		height: 90px;
		position: absolute;
		top: 0;
		left: 0;
}
100% {
		opacity: 0.6;
		width: 110px;
		height: 110px;
}
}
@-webkit-keyframes animation2 {
0% {
